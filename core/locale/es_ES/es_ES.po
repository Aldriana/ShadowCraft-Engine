msgid "total damage per second."
msgstr "daño total por segundo."

#: calcs/rogue/__init__.py:66
#, possible-python-format
msgid "No %(spell_name)s formula available for level %(level)d"
msgstr "No hay fórmula disponible para la habilidad %(spell_name)s a nivel %(level)d"

#: calcs/rogue/Aldriana/__init__.py:30
msgid "You must have 31 points in at least one talent tree."
msgstr "Debes tener 31 puntos en al menos una rama de talentos."

#: calcs/rogue/Aldriana/__init__.py:256
msgid "PPMs that also proc off spells are not yet modeled."
msgstr "Los efectos de proc por minuto que se activan mediante hechizos aún no están modelados"

#: calcs/rogue/Aldriana/__init__.py:372
msgid "You must specify an assassination cycle to match your assassination spec."
msgstr "Debes especificar un ciclo de asesinato que concuerde con tu especialización de asesinato."

#: calcs/rogue/Aldriana/__init__.py:374
msgid "Assassination modeling requires daggers in both hands"
msgstr "El modelo de asesinato requiere dagas en ambas manos."

#: calcs/rogue/Aldriana/__init__.py:377
msgid "Assassination modeling requires instant poison on one weapon and deadly on the other"
msgstr "El modelo de asesinato requiere veneno instantáneo en un arma y mortal en la otra."

#: calcs/rogue/Aldriana/__init__.py:383
msgid "Assassination modeling requires one point in Master Poisoner"
msgstr "El modelo de asesinato requiere un punto en Maestro Envenenador."

#: calcs/rogue/Aldriana/__init__.py:385
msgid "Assassination modeling requires three points in Cut to the Chase"
msgstr "El modelo de asesinato requiere tres puntos en Acortar."

#: objects/buffs.py:46
#: objects/stats.py:45
#, possible-python-format
msgid "No conversion factor available for level %(level)d"
msgstr "No hay factor de conversión disponible para nivel %(level)d"

#: objects/procs.py:136
#, possible-python-format
msgid "No data for proc '%(proc)s'"
msgstr "No hay datos para el proc '%(proc)s'"

#: objects/race.py:77
#, possible-python-format
msgid "Unsupported race %(race)s"
msgstr "La raza %(race)s no está soportada."

#: objects/race.py:81
#, possible-python-format
msgid "Unsupported class %(class)s"
msgstr "La clase %(class)s no está soportada."

#: objects/race.py:100
#, possible-python-format
msgid "Unsupported class/level combination %(class)s/%(level)d"
msgstr "La combinación de clase: %(class)s / nivel: %(level)d no está soportada."
